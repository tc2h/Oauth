<!-- views/success.ejs -->
<!doctype html>
<html>
  <head>
   
    <title>Google SignIn</title>
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"> <!-- load bootstrap css -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"> <!-- load fontawesome -->
      <style>
          body        { padding-top:70px; }
      </style>
    
  </head>
  <body style="background: rgb(207, 184, 133);">
    
    <div class="container">
      <div class="jumbotron" style="color: rgb(61, 59, 56);">
        
          <h1 class="text-primary  text-center" ><span class="fa fa-user"></span> Profile Information</h1>
          <div class="row">
            <div class="col-sm-6" >
                <div class="well">
                 
                        <p >
                            
                            <strong>Email</strong>: <%= user.emails[0].value %><br>
                            <strong>Name</strong>: <%= user.displayName %><br>
                          
                        </p>
                </div>
            </div>
        </div>
        
      </div>
    </div>
    <div class="container">
      <div class="jumbotron text-center text-primary">
          <h1><span class="fa fa-unlock"></span> LOG OUT</h1>
          <p></p>
          <a href="/logout" class="btn btn-danger"><span class="fa fa-unlock"></span> Sign out</a>
      </div>
    </div>
  </body>
</html>